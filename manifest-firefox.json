{
  "manifest_version": 3,
  "name": "Jenkins Security Extension",
  "version": "1.0",
  "description": "Cross-browser extension to ease maintainer's life with advisory preparation.",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest"
  ],
  "host_permissions": [
    "*://github.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}